{
  "compilerOptions": {
    "target": "es2016",
    "module": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": "./",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node16",
    "lib": [
      "ES5",
      "DOM"
    ],
    "types": [
      "cypress",
      "node",
      "cypress-xpath",
      "cypress-file-upload",
      "@shelex/cypress-allure-plugin"
    ]
  },
  "include": [
    "**/*.ts",
    "cypress/e2e/pages/baseClass/basePage.ts"
  ]
}